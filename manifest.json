{
  "name": "UdpWebviewApp",
  "description": "UdpWebview Chrome App.",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "focus-16.png", "128": "focus-128.png" },
  "permissions": [
    "background",
    "geolocation",
    "storage",
	  "webview"
  ],
  "sockets": {
    "udp": {
      "send": ["*"],
      "bind": ["*"]
    }
  }
}